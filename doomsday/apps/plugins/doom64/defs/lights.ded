# jDoom64: Lights

SkipIf -nodefaultfx;


#####====--- LIGHT SOURCE PROPERTIES ---====#####

# The default properties of all light-emitting objects are determined
# automatically by the engine.

Light {
  State = "COLU";
  Size = 0.6;
}

Light {
  State = "PLASBALL";
  Flags = nohalo;
}

Light {
  State = "PLASBALL2";
  Flags = nohalo;
}

Include "decorlights.ded"
